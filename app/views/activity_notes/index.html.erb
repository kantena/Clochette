<% @activity_notes.each do |note| %>
  <p>
    <%= label_tag ('Société : ') %>
    <%= note.customer.name %>
  </p>
  <p>
    <%= label_tag ('Nb jours à facturer : ') %>
    <%= note.working_days %>
  </p
  <%= link_to "Modifier", :controller => "activity_notes", :action => "edit", :id => note %>
  <%= link_to('Supprimer', activity_note_url(note),:method =>'delete',:confirm => 'Are you sure ?') %>
<% end %>